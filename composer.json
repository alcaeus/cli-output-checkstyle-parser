{
    "name": "alcaeus/cli-output-checkstyle-parser",
    "description": "Parses CLI output of tools and converts it to checkstyle XML format",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Braun",
            "email": "git@alcaeus.org"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/console": "^3.2|^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "bin": [
        "bin/cli-output-checkstyle-parser"
    ],
    "autoload": {
        "psr-4": {
            "Alcaeus\\CliOutputCheckstyleParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Alcaeus\\CliOutputCheckstyleParser\\": "tests/"
        }
    }
}
